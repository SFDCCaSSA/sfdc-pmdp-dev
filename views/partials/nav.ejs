<script type="text/javascript">
    $j(document).ready(function(){
		var searchText = getUrlParameter("keywords");
		$j('#contenido').highlight(searchText);
		$j('.tile-title').highlight(searchText);
		$j('.tile-description').highlight(searchText);
		$j("#vvTituloSearch").text(searchText);
		$j("#search").val(searchText);
		$j("#search-mobile").val(searchText);
        $j(".slds-btn-menu").click(function() {
            $j(".slds-btn-menu").show();
            $j(this).hide();
            $j(".th-header-menu").toggleClass("slds-hide");
		});
		
		$j("#search").keypress(function(e){
			var key = e.which;
			if(key == 13){
				buscarTodo($j("#search").val());
				return false;
			}
		});
		
		$j("#search-mobile").keypress(function(e){
			var key = e.which;
			if(key == 13){
				buscarTodo($j("#search").val());
				return false;
			}
		});
		
		$j("#search-desktop-icon").click(function(e){
			buscarTodo($j("#search").val());
		});
		$j("#search-mobile-icon").click(function(e){
			buscarTodo($j("#search").val());
		});
	});
	
	function buscarTodo(valor){
		console.log(valor);
		if(valor.length<3){
			alert("Se requiere minimo 3 caracteres para realizar la busqueda");
		}else{
			document.location.href = "/search?keywords="+valor;
		}
	}



	function getUrlParameter(name) {
		name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
		var regex = new RegExp('[\\?&]' + name + '=([^&#]*)');
		var results = regex.exec(location.search);
		return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
	};
</script>

